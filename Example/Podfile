platform :ios, '8.0'
ENV['SWIFT_VERSION'] = '4'

source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!
inhibit_all_warnings!
supports_swift_versions '4.2'

def logsene_pods 
  pod 'Logsene', :path => '../', :swift_version => '3.2'
end

target 'Logsene_Example' do
  logsene_pods
  pod 'CocoaLumberjack/Swift', '3.2'
end

target 'Logsene_Tests' do
  use_frameworks!
  pod 'Quick', '1.0.0'
  pod 'Nimble', '5.0.0' 
end
