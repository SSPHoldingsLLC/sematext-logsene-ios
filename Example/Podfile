ENV['SWIFT_VERSION'] = '4'

source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!
inhibit_all_warnings!
supports_swift_versions '>= 4.0'

target 'Logsene_Example' do
  pod 'Logsene', :path => '../'
  pod 'CocoaLumberjack/Swift'
end

target 'Logsene_Tests' do
  pod 'Logsene', :path => '../', :swift_version => '4.0'

  pod 'Quick'
  pod 'Nimble'
end
